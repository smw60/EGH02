    function deg_to_dms(hdms) {
        var d = Math.floor(hdms);
        var minfloat = (hdms - d) * 60;
        var m = Math.floor(minfloat);
        var secfloat = (minfloat - m) * 60;
        var s = Math.round(secfloat);
        if (s == 60) {
            m++;
            s = 0;
        }
        if (m == 60) {
            d++;
            m = 0;
        }
        return (d);
    }
